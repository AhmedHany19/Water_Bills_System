@model SubscribtionsViewModel
@{
    ViewData["Title"] = "Subscribtions";
}
<!-- Main content -->
<section class="content-wrapper">
    <div class=" mt-3 ">
        <h2> الاشتراكات</h2>
        <a class="btn btn-primary mt-3 mb-2 " asp-action="Create">اضافة اشتراك جديد</a>
    </div>

    <div>
        <table id="SubscribtionsTable" class="table text-black text-center table-bordered table-hover">
            <thead>
                <tr>
                    <th>رقم الاشتراك</th>
                    <th>رقم المشترك</th>
                    <th>الاسم</th>
                    <th>رقم الجوال</th>
                    <th>نوع العقار</th>
                    <th>عدد الوحدات</th>
                    <th>هل يوجد صرف صحي ؟</th>
                    <th>اخر قراءة عداد</th>
                    <th>ملاحظات</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.Subscriptions)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Subscriber_Id</td>
                        <td>@item.Subscriber.Name</td>
                        <td>@item.Subscriber.Mobile+</td>
                        <td>@item.Real_Estate_Type.Name</td>
                        <td>@item.No</td>
                        <td>@item.Is_There_Sanitation</td>
                        <td>@item.Last_Reading_Meter</td>
                        <td>@item.Comments</td>
                        <td>
                            <partial name="_ButtonPartial" model="@item.Id" />
                        </td>
                    </tr>
                }
            </tbody>
        </table>

    </div>



</section>
<!-- /.content -->

@section Scripts{
        <script type="text/javascript" src="~/modules/DataTable.js">
           
        </script>
}